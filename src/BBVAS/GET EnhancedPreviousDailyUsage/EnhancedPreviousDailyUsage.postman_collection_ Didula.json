					],
					"path": [
						"tmf-api",
						"usageManagement",
						"v4",
						"enhancedPreviousDailyUsage",
						"legacy",
						"subscriber"
					],
					"query": [
						{
							"key": "subscriberID",
							"value": "{{subscriberID}}",
							"description": "Subscriber ID (required)"
						},
						{
							"key": "billDate",
							"value": "{{billDate}}",
							"description": "Bill date (maps to usageDate)"
						},
						{
							"key": "monthIndex",
							"value": "{{monthIndex}}",
							"description": "Month index"
						},
						{
							"key": "fields",
							"value": "{{fields}}",
							"description": "Comma-separated list of fields to return"
						}
					]
				},
				"description": "Legacy endpoint for backward compatibility\nMaps old API parameters to TMF635 format\n\nQuery Parameters:\n- subscriberID: Subscriber ID (required)\n- billDate: Bill date (maps to usageDate)\n- monthIndex: Month index\n- fields: Comma-separated list of fields to return\n\nExample:\nGET /tmf-api/usageManagement/v4/enhancedPreviousDailyUsage/legacy/subscriber?subscriberID=94112179676&billDate=2020-11-20&monthIndex=11"
			},
			"response": []
		},
