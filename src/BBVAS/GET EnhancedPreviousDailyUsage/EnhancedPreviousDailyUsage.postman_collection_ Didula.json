{
	"info": {
		"_postman_id": "enhanced-previous-daily-usage-collection",
		"name": "EnhancedPreviousDailyUsage - TMF635 Usage Management API",
		"description": "Postman collection for EnhancedPreviousDailyUsage endpoints following TMF635 v4.0.0 standards",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
		"_exporter_id": "enhanced-previous-daily-usage"
	},
	"item": [
		{
			"name": "List Enhanced Previous Daily Usage Records",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "Accept",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "Authorization",
						"value": "Bearer {{access_token}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{base_url}}/tmf-api/usageManagement/v4/enhancedPreviousDailyUsage?fields=id,href,status,usageType,usageDate&relatedParty.id={{subscriberID}}&usageDate={{usageDate}}&monthIndex={{monthIndex}}",
					"host": [
						"{{base_url}}"
					],
					"path": [
						"tmf-api",
						"usageManagement",
						"v4",
						"enhancedPreviousDailyUsage"
					],
					"query": [
						{
							"key": "fields",
							"value": "id,href,status,usageType,usageDate",
							"description": "Comma-separated list of fields to return"
						},
						{
							"key": "relatedParty.id",
							"value": "{{subscriberID}}",
							"description": "Filter by subscriber ID"
						},
						{
							"key": "usageDate",
							"value": "{{usageDate}}",
							"description": "Filter by usage date (YYYY-MM-DD)"
						},
						{
							"key": "monthIndex",
							"value": "{{monthIndex}}",
							"description": "Filter by month index (0-11)"
						},
						{
							"key": "validFor.startDateTime",
							"value": "{{startDateTime}}",
							"description": "Filter by start date/time (ISO 8601)"
						},
						{
							"key": "validFor.endDateTime",
							"value": "{{endDateTime}}",
							"description": "Filter by end date/time (ISO 8601)"
						},
						{
							"key": "usageType",
							"value": "{{usageType}}",
							"description": "Filter by usage type"
						},
						{
							"key": "status",
							"value": "{{status}}",
							"description": "Filter by status"
						}
					]
				},
				"description": "List enhanced previous daily usage records with TMF635 filtering\n\nQuery Parameters:\n- fields: Comma-separated list of fields to return\n- relatedParty.id: Filter by subscriber ID\n- usageDate: Filter by specific date\n- monthIndex: Filter by month index (0-11)\n- validFor.startDateTime: Filter by start date/time\n- validFor.endDateTime: Filter by end date/time\n- usageType: Filter by usage type\n- status: Filter by status\n\nExample:\nGET /tmf-api/usageManagement/v4/enhancedPreviousDailyUsage?fields=id,href,status,usageType,usageDate&relatedParty.id=94112179676&usageDate=2020-11-20&monthIndex=11"
			},
			"response": []
		},
		{
			"name": "Get Enhanced Previous Daily Usage by ID",
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					},
